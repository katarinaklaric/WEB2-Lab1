<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" href="/styles.css">
        <title>Ticket</title>
    </head>
    <body>
        <div class="ticket-content">
            <h2 class="user">Korisnik: <%= username %></h2>
            <div class="all-data">
                <h1>Podaci o ulaznici</h1>
                <div class="ticket-data">
                    <h2>OIB: <%= oib %></h2>
                    <h2>Ime: <%= firstName %></h2>
                    <h2>Prezime: <%= lastName %></h2>
                    <h2>Kreirana: <%= createdAt %></h2>
                </div>
            </div>
        </div>
    </body>
</html>